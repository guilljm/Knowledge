<div class="col-4 d-flex justify-content-center align-items-center bg-green">
	<ul class="list-group list-unstyled">
		{% for theme in themes %}
			<li class="border-bottom">
				<a href="/theme/show?id={{theme.id}}" class="bg-green fs-3 p-4 text-color-clair list-group-item">
					{{theme.name}}
				</a>
			</li>
		{% endfor %}
	</ul>
</div>
